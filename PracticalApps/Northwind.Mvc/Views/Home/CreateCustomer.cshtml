@using Packt.Shared
@model Customer
<h2>@ViewData["Title"]</h2>


 @if (Model is not null)
{
    Customer customer = Model;
    <td>
        "Пользователь создан"
    </td>
    <td>
        @Html.DisplayFor(modelItem => customer.CustomerId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => customer.CompanyName)
    </td>
}
else
{
    <td>
        "Пользователь не создан "
    </td>
}


