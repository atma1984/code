{
    "Logging": {
        "LogLevel": {
            "Default": "Information", // Логирование по умолчанию для всех компонентов
            "Microsoft": "Information", // Логирование для всех компонентов Microsoft (включая ASP.NET Core)
            "Microsoft.AspNetCore": "Information", // Логи для ASP.NET Core
            "Microsoft.Hosting.Lifetime": "Information", // Логи для жизненного цикла хоста
            "System": "Information" // Логи для системных компонентов
        }
    },
    "AllowedHosts": "*",
    "Kestrel": {
        "EndpointDefaults": {
            "Protocols": "Http2"
        }
    }
}