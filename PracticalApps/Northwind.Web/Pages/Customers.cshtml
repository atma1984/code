@page
@model Northwind.Web.Pages.CustomersModel

<h1>Customers</h1>
<table class="table">
    <thead>
        <tr>
            <th>Company</th>
            <th>Contact</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Customers != null)
          {
              foreach (var c in Model.Customers)
              {
                <tr>
                    <td>@c.CompanyName</td>
                    <td>@c.ContactName</td>
                </tr>
        }
}
    </tbody>
</table>
