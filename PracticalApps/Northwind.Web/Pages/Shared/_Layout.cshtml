<!doctype html>
<html lang="en">
<head>
	<!-- Требуемые мета-теги -->
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
	<!-- Bootstrap CSS -->
	<link href="https://cdn.jsdelivr.NET/npm/bootstrap@5.1.0/dist/css/bootstrap.min.css"
		  rel="stylesheet" integrity="sha384-KyZXEAg3QhqLMpG8r+8fhAXLRk2vvoC2f3B09zV
Xn8CA5QIVfZOJ3BCsw2P0p/We" crossorigin="anonymous">
	<title>@ViewData["Title"]</title>
</head>
<body>
	<div class="container">
		@RenderBody()
		<hr />
		<footer>
			<p>Copyright &copy this is common data; 2021 -2025 @ViewData["Title"]</p>
		</footer>
	</div>
	<!-- JavaScript-код для включения таких функций, как карусель -->
	<script src="https://cdn.jsdelivr.NET/npm/bootstrap@5.1.0/
dist/js/bootstrap.bundle.min.js" integrity="sha384-
U1DAWAznBHeqEIlVSCgzq+c9gqGAJn5c/t99JyeKa9xxaYpSvHU5awsuZVVFIhvj"
			crossorigin="anonymous"></script>
	@RenderSection("Scripts", required: false)
</body>
</html>