{
    "Logging": {
        "LogLevel": {
            "Default": "None", // Уровень для всех логов
            "Microsoft": "None", // Для логирования всех Microsoft-компонентов
            "Microsoft.AspNetCore": "None", // Логирование для ASP.NET Core
            "Microsoft.AspNetCore.Mvc": "None", // Логирование для MVC
            "Microsoft.AspNetCore.HttpLogging": "Information", // Логирование для HttpLoggingMiddleware
            "Microsoft.AspNetCore.Server.Kestrel": "None", // Логирование для Kestrel сервера           
            "Microsoft.Hosting.Lifetime": "Information",
            "Microsoft.EntityFrameworkCore.Database.Command": "Debug"
        }
    },
    "AllowedHosts": "*"
}
